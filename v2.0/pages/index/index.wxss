/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

.container{
  padding-bottom: 30rpx;
}
/*上部*/
.header{
  position: relative;
  display: flex;
  width: 100%;
  height: 360rpx;
}
.cx-img-box{
  position: absolute;
  width: 100%;
  height: 100%;
  background-size: 100%;
  background-position:  center center;
  z-index:1;
}
.hader-main-box{
  position: absolute;
  height: 118rpx;
  z-index: 2;
  bottom: 0;
  padding-bottom: 14rpx;
  display: flex;
  color: #fff;
  width: 100%;

}
.cx-prize-box{
  width: auto;
  padding-left: 29rpx;
  padding-right: 16rpx;
}


.prize-num-text{
  display: flex;
  font-size: 26rpx;
  align-items: center;
  
}
/* .prize-num,.time-down{font-size: 50rpx;font-weight: bold;} */
.prize-num{
  font-size: 54rpx;
  font-weight: bold;
  line-height: 1;
  text-shadow: 2rpx 2rpx 8rpx rgba(0,0,0,0.5);
  margin-top: -5rpx;
}
.header-btn{
  width: 145rpx;
  height: 49rpx;
  overflow: hidden;
  border-radius: 30rpx;
  line-height: 48rpx;
  color: #fff;
  font-size: 26rpx;
  text-align: center;
  margin: 42rpx 9rpx 0 9rpx;
}

.rankBtn{
  border:1px solid #fff;
  background: transparent;
}
.inviteBtn{
  background: #ff504b;
}
.user-avatar-box{
  position: absolute;
  right: 32rpx;
  bottom: 16rpx;
  width: 84rpx;
  height: 84rpx;
  border:3rpx solid #fff;
  border-radius: 100%;
  overflow: hidden;
}
.user-share-box{
  position: absolute;
  right: 30rpx;
  bottom:92rpx;
  width:200rpx;
  height: 70rpx;
  line-height: 70rpx;
  color: #ee0a3b;
  background: #fedf30 url(https://i2.dd-img.com/assets/image/1539764664-ec4a3d8e46086e49-42w-42h.png) no-repeat 29rpx center;
  background-size: 42rpx;
  border-radius: 15rpx;
  box-shadow: 0rpx 6rpx 0rpx #ff8601;
  padding-left: 88rpx;
  box-sizing: border-box
}
.user-money-box{
  color: #fdfbce;
  font-size: 28rpx;
  position: absolute;
  right: 30rpx;
  bottom:200rpx;
  border-bottom: 1rpx solid #fdfbce;
}
.cx-text{
  margin:35rpx 0 0 30rpx;
  font-size: 30rpx;
  color:#fff;
}
.cx-options{margin-top: -5rpx;height: 455rpx;}
/*竞猜车型*/
.btn{
  padding: 0;
  border:none;
  outline: none;
  border-radius: 50rpx;
  box-shadow: 0 6rpx 0 #ffd289, 0 9rpx 2rpx rgba(181,49,44,.9);
  margin: 26rpx 30rpx;
  color:#7c4407;
}
.btn-text{
  display: block;
  height: 88rpx;
  line-height: 88rpx;
  background: #fbfbfb;
  font-size:30rpx;
  color: #7d4408;
  padding-left: 34rpx;
  text-align: left;
} 
.last-btn{margin-bottom: 66rpx;}
.submit-btn{
  margin-top: 40rpx; 
  border-radius: 12rpx;
  box-shadow: 0 6rpx 0 #db630c, 0 9rpx 2rpx #b6312c;
}
.submit-btn text{
  text-align: center;
  color:#d52925;
  background: #ffe8c2;
  padding-left:0;
}
.white-bg-btn{
  box-shadow: 0 6rpx 0 #ffd289, 0 18rpx 28rpx rgba(253,148,102,.9);
}
.white-bg-btn text{
  background: #fbfbfb;
  color:#ff5c4f;
}
.join-btn{
  box-shadow: 0 6rpx 0 #b5120d, 0 9rpx 2rpx #b6302d;
}
.join-btn .btn-text{
  background: #d52925;
  color:#fff;
  padding-left: 0;
  text-align: center;
}

.current-btn{
  box-shadow: 0 6rpx 0 #db630c, 0 9rpx 2rpx #b5312d;
}
.current-btn .btn-text{
  background: #ff9d20;
  color: #fff;
}
.correct-btn{
  box-shadow: 0 6rpx 0 #06a352, 0 9rpx 2rpx #b6302d;
}
.correct-btn .btn-text{
  background: #16d474;
  color: #fff;
}

